{
    "builds": [
      {
        "src": "config/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
      "env": {
      "PYTHON_VERSION": "3.10"
    }
    ],
    "routes": [
      {
        "src": "core/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "config/wsgi.py"
      }
    ]
  }
  